<view class="pd16 navigator">
    <view class="pd16 bg-white br4">
        <mbutton bind:tap="navigatorTo" data-url="pages/page_jumping_01/page_jumping_01" size="large" class="btn" type="info">Go to a New Page</mbutton>
        <mbutton bind:tap="navigatorBack" size="large" class="btn mt12" type="info">Return to Previous Page</mbutton>
        <mbutton bind:tap="exit" size="large" class="btn" type="info mt12">Close All Pages</mbutton>
    </view>
</view>
